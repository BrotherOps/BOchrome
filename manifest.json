{
  "name": "BOchrome",
  "description": "Chrome extention for BrotherOps",
  "version": "0.1.2",
  "manifest_version": 3,
  "action": {
    "default_icon": "img/bo_extension.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["activeTab", "scripting"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["img/*"],
      "matches": ["<all_urls>"]
    }
  ]
}